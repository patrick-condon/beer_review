<div class="admin-index">
  <h2 id="admin">Active Users List</h2>
  <ul id="users-list">
    <% @users.each do |user| %>
      <li class="user-link">
        <img src="<%= user.profile_photo %>" width="100px" height="100px"/> - <%= link_to user.username, user %>
      </li>
    <% end %>
  </ul>
  <%= button_to 'Beers Index', root_path, method: :get %>
</div>
